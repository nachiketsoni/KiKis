<style>
    * {
        padding: 0%;
        margin: 0%;
        box-sizing: border-box;
    }

    html,
    body {

        width: 100%;
        background-color: rgb(255, 255, 255);
    }

    #left {
        height: 100vh;
        width: 100vw;
        background-color: rgb(255, 255, 255);
    }

    #box1 {
        display: flex;
        position: relative;
    }

    #adds {
        display: flex;
      
        height: 40%;
        width: 100%;
        position: relative;
        margin-top: 4%;
        background-color: rgb(0, 0, 0);

    }

    .food {
        margin-left: 1.5%;
    }

    #foodlist {
        display: flex;
        flex-wrap: wrap;
        width: 90%;
        margin-left: 7%;
        margin-top: 4%;
        background-color: rgba(255, 255, 255, 0.212);
        border-radius: 12px;
        overflow: hidden;
    }

    .cards-list {
        z-index: 0;
        width: 100%;
        display: flex;
        justify-content: space-around;
        flex-wrap: wrap;
    }

    .card {
        margin: 30px auto;
        width: 300px;
        height: 400px;
        border-radius: 22px;
        cursor: pointer;
        transition: .4s;
        background-color: rgb(241, 241, 241);
        overflow: hidden;
    }

    .card .card_image {
        width: 100%;
        height: 60%;
        padding: 20px 20px;
        overflow: hidden;
        border-radius: 22px;
    }

    .card .card_image img {
        height: 100%;
        width: 100%;
        overflow: hidden;
    }

    .card .card_title {

        border-radius: 0px 0px 40px 40px;
        font-family: sans-serif;
        font-size: 30px;
        margin-top: 0px;
        font-weight: 100;
        height: 40px;
        margin-left: 8%;
        color: rgb(94, 94, 94);
        font-size: 1.5vw;
    }

    #rating {
        position: relative;
        height: 3.4vh;
        width: 4vw;
        background-color: orange;
    }

    #rating h5 {
        position: absolute;
        right: 8%;
        top: 5%;
        color: whitesmoke;
    }

    #time {
        position: relative;
        left: 40%;
        top: -50%;
        color: whitesmoke;
        color: black;

    }

    #time h6 {
        font-size: 1vw;
        font-weight: 100;
    }

    #paisa {
        position: relative;
        height: 2vh;
        width: 4vw;
        left: 76%;
        top: -150%;
        color: whitesmoke;
        color: black;


    }

    #paisa h6 {
        font-size: 1.1vw;
        font-weight: 100;
    }

    .card .card_title h6 {

        border-radius: 0px 0px 40px 40px;
        font-family: sans-serif;
        font-size: 30px;
        margin-top: 0px;
        font-weight: 100;
        height: 40px;

        color: rgb(128, 128, 128);
        font-size: 1.2vw;
    }

    #line {
        height: 0.1vh;
        margin-top: -11%;
        width: 90%;
        background-color: rgb(199, 199, 199);
    }

    #welcome {
        font-size: 0.9vw;
        color: rgb(158, 158, 158);
        margin-top: 5%;
    }

    #welcome a {
        margin-left: 30%;
        text-decoration: none;
    }

    .card:hover {
        transform: scale(0.9, 0.9);
        border: 1px solid rgb(255, 255, 255);
    }

    .title-white {
        color: white;
    }

    .title-black {
        color: black;
